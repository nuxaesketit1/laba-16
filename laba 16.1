#include <iostream>
#include <cmath>
using namespace std;

int main() {

	setlocale(0, "");

	int n, k = 0;

	do {

		cout << "Введите количество элементов массива N (N > 0) : ";
		cin >> n;
		cout << endl;

	} while (n<0);

	int* array = new int[n];

	cout << "Введите элементы массива : ";

	for (int i = 0; i < n; i++)
		cin >> array[i];

	for (int i = 1; i < n; i++) {
		if (array[k] != array[i]) {
			k++;
			array[k] = array[i];
		}
	}

	cout << endl;
	cout << "Результат : " << endl;

	for (int i = 0; i < (k + 1); i++)
		cout << array[i] << " ";

	cout << endl;
	system("pause");
	return 0;

}
