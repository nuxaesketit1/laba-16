#include <iostream>
#include <cmath>
using namespace std;

int main() {

	setlocale(0, "");

	int n, num;

	do {

		cout << "Введите количество элементов массива N (N > 0) : ";
		cin >> n;
		cout << endl;

	} while (n<0);

	int* array = new int[n];

	cout << "Введите элементы массива : ";

	for (int i = 0; i < n; i++)
		cin >> array[i];

	for (int i = 0; i < n; i++) {
		num = 0;
		for (int j = 0; j < n; j++)
			if (array[i] == array[j]) 
				num++;
		if (num == 2) {
			for (int j = n-1; j >= 0; j--)
				if (array[i] == array[j]) {
					n--;
					for (int y = j; y < n; y++)
						array[y] = array[y + 1];
				}
		}
	}

	cout << endl;
	cout << "Результат : " << endl;

	for (int i = 0; i < n; i++)
		cout << array[i] << " ";

	cout << endl;
	system("pause");
	return 0;

}
